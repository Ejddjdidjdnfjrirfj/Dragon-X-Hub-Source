local DH = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()
local Player = game.Players.LocalPlayer

local Window = DH:Window("Dragon Hub", "Troll Vertion", Color3.fromRGB(255, 5, 5), Enum.KeyCode.LeftControl)
local Main = Window:Tab("Main", "http://www.roblox.com/asset/?id=6023426915")
Main:Label("User: "..Player.Name.."")
Main:Label("Map: "..game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name.."")
